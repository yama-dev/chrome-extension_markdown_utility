{
  "manifest_version": 2,
  "name": "Markdown Utility.",
  "version": "0.0.1",
  "description": "Markdown Utility.",
  "author": "yama-dev",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://bitbucket.org/*/issues/*","https://github.com/*/issues/*"],
      "js": ["repalace_html-tags.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
